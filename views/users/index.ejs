<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/stylesheets/mult-pages.css" />
    <link rel="stylesheet" href="/stylesheets/index.css" />

    <title>Community</title>
  </head>
  <body>
    <%- include('../partials/_communityNavBar.ejs') %>

    <h1>TriTracker Community</h1>

    <ul>
      <% users.forEach(user => { %>
      <li>
        <a href="/users/<%= user._id %>"><%= user.username %></a>
      </li>
      <% }); %>
    </ul>
  </body>
</html>
